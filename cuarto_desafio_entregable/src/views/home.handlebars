<h1>LISTA DE PRODUCTOS</h1>
<div class="productsContainer">
  {{#each products}}
    <div class="product">
      <h4>{{this.title}}</h4>
      <p>$ {{this.price}}.-</p>
      <img
        src={{this.thumbnail}}
        alt="imagen producto"
        width="120px"
        height="120px"
      />
    </div>
  {{/each}}

</div>